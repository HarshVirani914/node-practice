{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES6"],
    "module": "CommonJS",
    "rootDir": "",
    "resolveJsonModule": true,
    "outDir": "build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
